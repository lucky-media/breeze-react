{
    "name": "lucky-media/breeze-react",
    "description": "Minimal Laravel authentication scaffolding with Blade and Tailwind.",
    "keywords": [
        "laravel",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lucky-media/breeze-react/issues",
        "source": "https://github.com/lucky-media/breeze-react"
    },
    "authors": [
        {
            "name": "Lokman Musliu",
            "email": "lokman.musliu@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "main"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
